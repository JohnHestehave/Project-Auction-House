<Window x:Class="Project_Auction_House_Client.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Project_Auction_House_Client"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
	<Grid FocusManager.FocusedElement="{Binding ElementName=IPTextBox}">
		<Button x:Name="LoginButton" Content="Login" HorizontalAlignment="Left" Margin="245,140,0,0" VerticalAlignment="Top" Width="75" Height="40" Click="Login_Click"/>
		<Label x:Name="IPLabel" Content="IP:" HorizontalAlignment="Left" Margin="121,98,0,0" VerticalAlignment="Top"/>
		<TextBox x:Name="IPTextBox" HorizontalAlignment="Left" Height="23" Margin="164,102,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="156" KeyDown="Login_EnterKey" TabIndex="1"/>

	</Grid>
</Window>
